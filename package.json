    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-intersection-observer": "^9.0.0",
    "react-redux": "^9.0.0",
    "react-router-dom": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@eslint/js": "^9.0.0",
    "@jest/globals": "^30.0.0",
    "@storybook/addon-links": "^9.0.0",
    "@storybook/addon-webpack5-compiler-swc": "^4.0.0",
    "@storybook/react": "^9.0.0",
    "@storybook/react-webpack5": "^9.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/webpack-env": "^1.0.0",
    "babel-jest": "^30.0.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.0.0",
    "cypress": "^15.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "globals": "^16.0.0",
    "html-webpack-plugin": "^5.0.0",
    "jest": "^30.0.0",
