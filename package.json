    "storybook": "^9.0.0",
    "style-loader": "^4.0.0",
    "stylelint": "^16.0.0",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-standard": "^39.0.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "url-loader": "^4.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^6.0.0",
    "webpack-dev-server": "^5.0.0"
