{
  "name": "stellar-burgers",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "webpack --mode=production",
    "start": "webpack serve --mode=development",
    "test-e2e:run-app": "webpack serve --mode=production",
    "test-e2e:open-cypress": "cypress open --e2e --env BURGER_API_URL=http://localhost:5000",
    "storybook:start": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "format": "prettier --write .",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "eslint .",
    "lint:styles": "stylelint '**/*.{css,scss}'",
    "lint-fix": "npm run lint-fix:ts && npm run lint-fix:styles",
    "lint-fix:ts": "eslint --fix .",
    "lint-fix:styles": "stylelint --fix '**/*.{css,scss}'"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.0",
    "@zlden/react-developer-burger-ui-components": "^1.0.0",
